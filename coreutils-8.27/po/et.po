msgstr "vigane universaal sümboli nimi \\%c%0*x"

#: src/printf.c:566
#, c-format
msgid "invalid field width: %s"
msgstr "vigane välja laius: %s"

#: src/printf.c:601
#, c-format
msgid "invalid precision: %s"
msgstr "vigane täpsus: %s"

#: src/printf.c:628
#, c-format
