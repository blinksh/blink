"  \\\\      backslash\n"
"  \\a      alert (BEL)\n"
"  \\b      backspace\n"
"  \\c      produce no further output\n"
"  \\e      escape\n"
"  \\f      form feed\n"
"  \\n      new line\n"
"  \\r      carriage return\n"
"  \\t      horizontal tab\n"
"  \\v      vertical tab\n"
msgstr ""
"  \\\\     een backslash\n"
"  \\a     geluidssignaal\n"
"  \\b     backspace\n"
"  \\c     geen verdere uitvoer produceren\n"
"  \\e     escape\n"
"  \\f     nieuwe pagina (FF-teken)\n"
"  \\n     nieuwe regel (LF-teken)\n"
"  \\r     naar begin van regel (CR-teken)\n"
"  \\t     horizontale tab\n"
"  \\v     verticale tab\n"

#: src/echo.c:77
msgid ""
"  \\0NNN   byte with octal value NNN (1 to 3 digits)\n"
